@prefix : <http://www.semanticweb.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix agentrole: <http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl#> .
@prefix aquaticresources: <http://www.ontologydesignpatterns.org/cp/owl/fsdas/aquaticresources.owl#> .
@prefix climaticzone: <http://www.ontologydesignpatterns.org/cp/owl/fsdas/climaticzone.owl#> .
@prefix collectionentity: <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#> .
@prefix partof: <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#> .
@prefix linnaeantaxonomy: <http://www.ontologydesignpatterns.org/cp/owl/linnaeantaxonomy.owl#> .
@prefix specieseat: <http://www.ontologydesignpatterns.org/cp/owl/fsdas/specieseat.owl#> .

:YearRange rdf:type owl:Class .

:hasYear rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf (:PhDStudent :AquaticResourceObservation)
                      ] ;
          rdfs:range :Year .

:earliestYear rdf:type owl:DatatypeProperty ;
               rdfs:domain :YearRange ;
               rdfs:range xsd:gYear .

:latestYear rdf:type owl:DatatypeProperty ;
             rdfs:domain :YearRange ;
             rdfs:range xsd:gYear .

:hasYearRange rdf:type owl:ObjectProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf (:University :AquaticResource)
                          ] ;
              rdfs:range :YearRange .

:YearRangeWithPhDStudents rdf:type owl:Class ;
                          rdfs:subClassOf :YearRange ;
                          owl:equivalentClass [
                              rdf:type owl:Class ;
                              owl:intersectionOf (
                                  :YearRange
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :earliestYear ;
                                    owl:hasValue [
                                        rdf:type owl:Restriction ;
                                        owl:onProperty :hasYear ;
                                        owl:someValuesFrom :PhDStudent
                                    ]
                                  ]
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :latestYear ;
                                    owl:hasValue [
                                        rdf:type owl:Restriction ;
                                        owl:onProperty :hasYear ;
                                        owl:someValuesFrom :PhDStudent
                                    ]
                                  ]
                              )
                          ] .

:YearRangeWithAquaticResourceObservations rdf:type owl:Class ;
                                          rdfs:subClassOf :YearRange ;
                                          owl:equivalentClass [
                                              rdf:type owl:Class ;
                                              owl:intersectionOf (
                                                  :YearRange
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :earliestYear ;
                                                    owl:hasValue [
                                                        rdf:type owl:Restriction ;
                                                        owl:onProperty :hasYear ;
                                                        owl:someValuesFrom :AquaticResourceObservation
                                                    ]
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :latestYear ;
                                                    owl:hasValue [
                                                        rdf:type owl:Restriction ;
                                                        owl:onProperty :hasYear ;
                                                        owl:someValuesFrom :AquaticResourceObservation
                                                    ]
                                                  ]
                                              )
                                          ] .
