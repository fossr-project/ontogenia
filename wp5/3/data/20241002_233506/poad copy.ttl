@prefix agentrole: <http://www.ontologydesignpatterns.org/cp/owl/agentrole.owl#> .
@prefix aquaticresources: <http://www.ontologydesignpatterns.org/cp/owl/fsdas/aquaticresources.owl#> .
@prefix climaticzone: <http://www.ontologydesignpatterns.org/cp/owl/fsdas/climaticzone.owl#> .
@prefix collectionentity: <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#> .
@prefix partof: <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#> .
@prefix linnaeantaxonomy: <http://www.ontologydesignpatterns.org/cp/owl/linnaeantaxonomy.owl#> .
@prefix specieseat: <http://www.ontologydesignpatterns.org/cp/owl/fsdas/specieseat.owl#> .

:DepartmentNamingConvention rdf:type owl:Class ;
                            rdfs:subClassOf :Department .

:hasNamingConvention rdf:type owl:ObjectProperty ;
                     rdfs:domain :Department ;
                     rdfs:range :DepartmentNamingConvention .

:DepartmentNamingVariant rdf:type owl:Class ;
                         rdfs:subClassOf :DepartmentNamingConvention .

:hasVariant rdf:type owl:ObjectProperty ;
            rdfs:domain :DepartmentNamingConvention ;
            rdfs:range :DepartmentNamingVariant .

:usesAbbreviation rdf:type owl:DatatypeProperty ;
                  rdfs:domain :DepartmentNamingConvention ;
                  rdfs:range xsd:boolean .

:isCaseSensitive rdf:type owl:DatatypeProperty ;
                 rdfs:domain :DepartmentNamingConvention ;
                 rdfs:range xsd:boolean .

:hasExactMatch rdf:type owl:ObjectProperty ;
               owl:propertyChainAxiom ( :hasVariant :hasVariant ) ;
               rdfs:domain :DepartmentNamingVariant ;
               rdfs:range :DepartmentNamingVariant .

:DepartmentWithMultipleNamingConventions rdf:type owl:Class ;
                                         owl:equivalentClass [
                                             rdf:type owl:Class ;
                                             owl:intersectionOf (
                                                 :Department
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasNamingConvention ;
                                                   owl:minCardinality "2"^^xsd:nonNegativeInteger
                                                 ]
                                             )
                                         ] .
